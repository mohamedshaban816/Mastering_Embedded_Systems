#include <stdio.h>

int main(void)
{
	float arr1[50][50];
	float arr2[50][50];
	int i,j;
	printf("Enter the first array elements\n");
	fflush(stdout);		fflush(stdin);
	printf("Enter the first array elements\n");
	fflush(stdout);		fflush(stdin);
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			printf("Enter the element %d%d: ",i,j);
			fflush(stdout);		fflush(stdin);
			scanf("%f",&arr1[i][j]);
		}

	}

	return 0;
}
