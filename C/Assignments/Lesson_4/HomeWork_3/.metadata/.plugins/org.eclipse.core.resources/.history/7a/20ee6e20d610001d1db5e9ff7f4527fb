#include <stdio.h>

int main(void)
{
	float arr1[2][2];
	float arr2[2][2];
	int i,j;
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			printf("Enter the element %d%d",i,j);
			fflush(stdout);		fflush(stdin);
		}

	}
	return 0;
}
